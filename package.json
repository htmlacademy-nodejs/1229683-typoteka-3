{
  "name": "typoteka",
  "version": "1.0.0",
  "description": "«Куплю. Продам»",
  "main": "./src/service/service.js",
  "scripts": {
    "start": "cross-env LOG_LEVEL=error node -r dotenv/config ./src/service/service.js",
    "start:dev": "cross-env LOG_LEVEL=error nodemon -r dotenv/config ./src/service/service.js --server",
    "start::debug": "cross-env LOG_LEVEL=info NODE_ENV=development node -r dotenv/config ./src/service/service.js --server",
    "start-front-server": "nodemon -r dotenv/config ./src/express/index.js",
    "test": "cross-env NODE_ENV=test  jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run db:reset",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "eslint": "eslint --ext .js src/ --fix",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run migrate && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klimnikova/1229683-typoteka-3.git"
  },
  "author": "Maria Klimnikova",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/klimnikova/1229683-typoteka-3/issues"
  },
  "homepage": "https://github.com/klimnikova/1229683-typoteka-3#readme",
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-config-htmlacademy": "0.5.2",
    "jest": "26.5.2",
    "nodemon": "2.0.2",
    "pino-pretty": "4.3.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "5.0.0"
  },
  "dependencies": {
    "axios": "0.20.0",
    "chalk": "3.0.0",
    "cross-env": "7.0.2",
    "dayjs": "1.8.20",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "joi": "17.4.2",
    "multer": "1.4.2",
    "nanoid": "3.1.7",
    "pg": "8.6.0",
    "pino": "6.7.0",
    "pug": "2.0.4",
    "sequelize": "6.6.2"
  }
}
